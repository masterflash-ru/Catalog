{
    "name": "masterflash-ru/catalog",
    "description": "Универсальный каталог товара",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1"
        "masterflash-ru/ado": "^2.0",
        "masterflash-ru/storage": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Catalog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Catalog\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev",
            "dev-develop": "0.0.1-dev"
        },
        "zf": {
            "component": "Mf\\Catalog",
            "config-provider": "masterflash-ru\\Mf\\Catalog\\ConfigProvider"
        }
    }
}
