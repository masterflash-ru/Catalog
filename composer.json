{
    "name": "masterflash-ru/catalog",
    "description": "Универсальный каталог товара",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "masterflash-ru/ado": "^2.2",
        "masterflash-ru/storage": "^1.2",
        "masterflash-ru/navigation": "*",
        "laminas/laminas-paginator": "^2.7",
        "laminas/laminas-cache": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Catalog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Catalog\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev",
            "dev-develop": "0.0.1-dev"
        },
        "zf": {
            "component": "Mf\\Catalog",
            "config-provider": "masterflash-ru\\Mf\\Catalog\\ConfigProvider"
        }
    }
}
